/* rs-theme-name: dark alley */
/* rs-theme-is-dark: TRUE */

:root {
  /* editor */
  --editor-background-color: 0, 0, 0; /* Pure black */
  --editor-background: rgb(var(--editor-background-color));
  --editor-foreground: rgb(220, 220, 220); /* Slightly lighter gray for good contrast*/

  --separator: rgb(30, 30, 30); /* Very dark gray*/
  --toolbar: rgb(15, 15, 15);  /* Darker Toolbar */
  --tab: rgb(20, 20, 20);   /* Slightly lighter than toolbar*/
  --selected-tab: var(--editor-background); /* Stays black */

  /* syntax */
  --type-declaration: #7fffd4;  /* Aqua / Bright Teal */
  --language-constant: #ff4500; /* Orange Red*/
  --numeric-constant: #ff4500; /* Orange Red */
  --string: #90ee90;      /* Light Green */
  --regex: #ff69b4;  /* Hot Pink */
  --keyword: #dda0dd;   /* Plum/Thistle */
  --function-declaration: #6495ed; /* Cornflower Blue */
  --variable: #ffffff;    
  --comment: #555555;   
  --operator: #ee82ee;   /* Violet */

  /* markdown */
  --heading: var(--language-constant); /* Orange-red */
  --inline-code: var(--string);  /* Light green */
}

.ace_gutter {
  color: rgb(100, 100, 100); /* Slightly darker gray */
}

.ace_print-margin {
  width: 2px;
  box-shadow: 1px 0 0 0 rgb(70, 70, 70) inset; /* Darker shadow */
}

.ace_editor,
.rstudio-themes-flat.ace_editor_theme .profvis-flamegraph,
.rstudio-themes-flat.ace_editor_theme,
.rstudio-themes-flat .ace_editor_theme {
  background-color: var(--editor-background);
  color: var(--editor-foreground);
  line-height: 1.3 !important;
}

.ace_cursor {
  border-color: rgb(190, 190, 190); /* Lighter, visible cursor */
  width: 2px;
}

.ace_marker-layer .ace_selection {
  background-color: #202020; /* Even darker selection highlight */
}

.ace_selection.ace_start {
  box-shadow: 0 0 3px 0px #101010; /*Darker shadow*/
  border-radius: 2px;
}

.ace_marker-layer .ace_step {
  background: rgb(110, 90, 0); /* Slightly brighter step marker*/
}

.ace_marker-layer .ace_bracket {
  outline: 1px solid rgb(120, 120, 120); /* Darker bracket color*/
  background-color: rgba(0, 100, 0, 0.1); /*Kept from original*/
}

.ace_marker-layer .ace_active-line {
  outline: 2px solid rgb(30, 30, 30); /* Darker active line color*/
}

.ace_gutter-active-line {
  background-color: rgb(170, 170, 170); /* Slightly Lighter */
}

.ace_marker-layer .ace_selected-word {
  background-color: rgba(234, 92, 0, 0.50); /*Slightly more opaque*/
}

.ace_invisible {
  color: rgba(227, 228, 226, 0.1); /*Darker, less visible*/
}

.ace_keyword:not(.ace_operator),
.ace_meta,
.ace_storage,
.ace_storage.ace_type,
.ace_support.ace_type {
  color: var(--keyword);
}

.ace_keyword.ace_operator {
  color: var(--operator);
}

.ace_constant.ace_character,
.ace_keyword.ace_other.ace_unit,
.ace_support.ace_constant,
.ace_variable.ace_parameter {
  color: var(--language-constant);
}

.ace_constant.ace_language,
.ace_keyword.ace_virtual-comment {
  color: var(--language-constant);
}

.ace_constant.ace_numeric,
.ace_constant.ace_other {
  color: var(--numeric-constant);
}

.ace_invalid {
  color: #101010; /*almost black*/
  background-color: #ff4500; /*Orange-red*/
}

.ace_invalid.ace_deprecated {
  color: #101010; /*almost black*/
  background-color: #dda0dd; /*Plum/Thistle*/
}

.ace_gutter-cell:not([style*="top"]) {
  position: relative;
}

.ace_line .ace_fold {
  background: none;
  height: inherit;
  margin: 0;
  border: none;
  position: relative;
}

.ace_line .ace_fold::before {
  color: grey;
  content: "â‹¯";
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
}

.ace_gutter .ace_fold-widget {
  cursor: pointer;
  background: no-repeat border-box transparent;
  border: none;
  border-radius: 0;
  box-shadow: none;
  width: 17px;
  position: absolute;
  top: 50%;
  right: 0;
  transform: translate(-50%, -50%);
}

.ace_gutter .ace_fold-widget.ace_closed {
  background-image: url("data:image/svg+xml;charset=utf-8,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 15 15'%3E%3Cpath opacity='.1' fill='%23fff' d='M3 3h9v9H3z'/%3E%3Cpath d='M11 4v7H4V4h7m1-1H3v9h9V3z' fill='%235a5a5a'/%3E%3Cpath fill='none' stroke='%23c5c5c5' stroke-miterlimit='10' d='M10 7.5H5M7.5 5v5'/%3E%3C/svg%3E");
}

.ace_gutter .ace_fold-widget.ace_open {
  background-image: url("data:image/svg+xml;charset=utf-8,%3Csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 15 15'%3E%3Cpath d='M11 4v7H4V4h7m1-1H3v9h9V3z' fill='%235a5a5a'/%3E%3Cpath fill='none' stroke='%23c5c5c5' stroke-miterlimit='10' d='M10 7.5H5'/%3E%3C/svg%3E");
  opacity: 0;
  transition: opacity 0.5s;
}

.ace_gutter:hover .ace_fold-widget.ace_open {
  opacity: 1;
}

.ace_entity.ace_name.ace_function,
.ace_identifier.ace_support.ace_function:not(.ace_virtual-comment) {
  color: var(--function-declaration);
}

.ace_identifier,
.ace_variable {
  color: var(--variable);
}

.ace_support.ace_class,
.ace_support.ace_type {
  color: var(--type-declaration);
}

.ace_heading,
.ace_markup.ace_heading {
  color: var(--heading);
}

.ace_markup.ace_heading .ace_entity.ace_name.ace_tag,
.ace_entity.ace_other.ace_attribute-name,
.ace_meta.ace_tag,
.ace_string {
  color: var(--string);
}

.ace_string.ace_regexp {
  color: var(--regex);
}

/* Comments */
.ace_comment {
  color: #666666; /* Dark gray */
}

/* Rmarkdown code */
[class="ace_support ace_function"] {
    color: #00FF7F; /* Neon green */
}

/* Rmarkdown emphasis */
.ace_constant.ace_language.ace_boolean.ace_text {
  font-style: italic;
    color: #00FFFF; /* Light Cyan */
}

/* Rmarkdown strong emphasis */
.ace_constant.ace_numeric.ace_text {
  font-weight: bold;
  color: #FF00FF; /* Neon Magenta */
}

/* Indent guides */
.ace_indent-guide {
  box-shadow: 1px 0 0 0 #222222; /* Very dark gray */
  display: inline-block;
}

/* nocolor text */
.nocolor.ace_editor .ace_line span {
  color: #cccccc !important; /* Light gray */
}

/* Foreign line background */
.ace_marker-layer .ace_foreign_line {
  position: absolute;
  z-index: -1;
  background-color: rgba(255, 255, 255, 0.04); /* Very subtle white for highlight */
}

/* Active debug line */
.ace_marker-layer .ace_active_debug_line {
  position: absolute;
  width: 100% !important;
  z-index: -1;
  background-color: rgba(255, 255, 0, 0.3); /* Yellow with more intensity */
}

/* Find line background */
.ace_marker-layer .ace_find_line {
  position: absolute;
  z-index: -1;
  background-color: #1a1a1a; /* Very dark gray */
}

/* Console error */
.ace_console_error {
  color: #ff4d4d; /* Bright red */
  background-color: #1a1a1a; /* Very dark gray */
}

/* Console keyword */
#rstudio_console_output .ace_keyword {
    color: #33aaff; /* Bright Blue */
}

/* Console constant language */
#rstudio_console_output .ace_constant.ace_language {
  color: #ff4d4d; /* Bright red */
}


/* Links */
.rstudio-themes-flat.editor_dark.ace_editor_theme a {
  color: #00FFFF !important; /* Light cyan */
}

.ace_layer {
  z-index: 3;
}

.ace_layer.ace_print-margin-layer {
  z-index: 2;
}

.ace_layer.ace_marker-layer {
  z-index: 1;
}

/* Autocomplete */
.rstudio-themes-flat.rstudio-themes-dark-menus .ace_editor.ace_autocomplete {
    background: #0a0a0a; /* almost black */
  border: solid 1px #333333 !important; /* Dark gray border */
  color: #e6e6e6; /* Light gray text */
}

.rstudio-themes-flat.rstudio-themes-dark-menus .ace_editor.ace_autocomplete .ace_marker-layer .ace_active-line,
.rstudio-themes-flat.rstudio-themes-dark-menus .ace_editor.ace_autocomplete .ace_marker-layer .ace_line-hover {
    background: rgba(100, 100, 100, 0.35); /* Darker gray */
  border: none;
}

.rstudio-themes-flat > .rstudio-themes-dark div:last-child[style*="top: 29px"] {
  top: 24px !important;
  right: 0 !important;
  bottom: 0 !important;
  left: 0 !important;
}

.rstudio-themes-flat > .rstudio-themes-dark div:last-child[style*="top: 5px"] {
  top: 0 !important;
  right: 0 !important;
  bottom: 0 !important;
  left: 0 !important;
}

.rstudio-themes-dark
  :-webkit-any(div:last-child[style*="top: 5px"], div:last-child[style*="top: 29px"])
  .gwt-SplitLayoutPanel-Workbench {
  top: 0 !important;
  right: 0 !important;
  bottom: 0 !important;
  left: 0 !important;
}

/* Background */
.rstudio-themes-flat > .rstudio-themes-dark,
.rstudio-themes-flat > .rstudio-themes-dark :-webkit-any(.gwt-TabLayoutPanel, .gwt-TabLayoutPanelContent) {
  background: #000000 !important; /* Solid black */
}

/* Splitters */
.rstudio-themes-flat > .rstudio-themes-dark .gwt-SplitLayoutPanel-HDragger {
  background: #000000 !important; /* Solid black */
  cursor: ew-resize;
  border-color: #000000; /* Solid black */
}

.rstudio-themes-flat > .rstudio-themes-dark .gwt-SplitLayoutPanel-VDragger {
  background: #000000 !important; /* Solid black */
  cursor: ns-resize;
  border-color: #000000; /* Solid black */
}

/* Window containers */
.rstudio-themes-flat
  > .rstudio-themes-dark
  :-webkit-any(.windowframe, .rstheme_minimizedWindowObject)
  > div:last-child {
  border-radius: 0;
  border: none !important;
}

/* Main toolbar */
.rstudio-themes-dark > div:last-child > div > div > .rstheme_toolbarWrapper,
.rstudio-themes-dark [role="application"] > div > .rstheme_toolbarWrapper {
  background-color: #0a0a0a !important; /* Almost black toolbar */
  border-color: #0a0a0a !important;
}


/* other toolbars */
.rstudio-themes-dark .rstheme_toolbarWrapper,
.rstudio-themes-dark .rstheme_secondaryToolbar {
  background-color: #0a0a0a !important; /* almost black */
  border-color: #0a0a0a !important; /* almost black */
}

/* other toolbars */
.rstudio-themes-dark .rstudio-themes-background {
  --color: #1a1a1a; /* Very dark gray */
  background-color: var(--color) !important;
  border-top-color: var(--color) !important;
  border-bottom-color: var(--color) !important;
  border-radius: 0;
}

/* Files breadcrumb */
.rstudio-themes-dark .breadcrumb > div:last-child {
  background: #0a0a0a !important; /* almost black */
  left: inherit !important;
}

/* Tabs container */
.rstudio-themes-dark .gwt-TabLayoutPanelTabs,
/* Windows */
.rstudio-themes-dark .windowframe,
/* Remove gradients */
.rstudio-themes-dark :-webkit-any(.rstheme_multiPodUtilityTabArea, .GD15MCFCCS, .GJQ3LUQCCS) {
    background: #0a0a0a !important; /* Almost black */
  border-color: #0a0a0a !important; /* Almost black */
}


/* Tabs */
.rstudio-themes-dark .gwt-TabLayoutPanelTab:not(.gwt-TabLayoutPanelTab-selected) .rstheme_tabLayoutCenter,
.rstudio-themes-dark .rstheme_minimizedWindowObject .rstheme_center {
  background-color: #1a1a1a !important; /* Very dark gray */
  color: #cccccc !important; /* Light gray */
  border-color: #1a1a1a !important;
  border-right: 1px solid #0a0a0a !important; /* almost black */
  border-radius: 0;
  cursor: pointer;
}

.rstudio-themes-dark .gwt-TabLayoutPanelTab .gwt-Label {
  cursor: pointer;
}

/* Active tab */
.rstudio-themes-dark .gwt-TabLayoutPanelTab.gwt-TabLayoutPanelTab-selected .rstheme_tabLayoutCenter {
    background-color: #333333 !important; /* slightly lighter grey for active tab */
  color: #ffffff !important; /* White text */
}
/* Comments */
.ace_comment {
  color: #808080; /* Dark gray, typical comment color */
}

/* Editor */
#rstudio_shell_widget.ace_editor.ace_scroller,
#rstudio_source_text_editor.ace_editor :-webkit-any(.ace_gutter, .ace_scroller, .ace_scrollbar-v) {
  box-shadow: inset 0 6px 6px -6px #222; /* Darker shadow for subtle depth */
}

/* Datagrid */
.rstudio-themes-dark .dataGridHeader,
.rstudio-themes-dark tr[__gwt_header_row] > :-webkit-any(td, th),
.rstudio-themes-dark .dataTables_info {
  background-color: #1a1a1a !important; /* Near black background */
}

/* Data Table */
.rstudio-themes-dark .dataTable :-webkit-any(th, .first-child) {
  background-color: #1a1a1a !important; /* Near black background for table headers */
}

.rstudio-themes-dark .dataTable :-webkit-any(th, td, tr) {
  border-color: #333 !important; /* Dark gray border color */
}

.rstudio-themes-dark .dataTable tbody > tr:hover > td {
  background-color: #222 !important; /* Slightly lighter near-black for hover */
}

/* Search background */
.rstudio-themes-dark .gwt-TabLayoutPanelContent > div:nth-last-child(2):nth-child(4) .rstudio-themes-background {
  background-color: #111 !important; /* Use #111 for toolbar background */
}

/* Search input */
.rstudio-themes-dark :-webkit-any(.rstheme_toolbarWrapper, #rstudio_find_replace_bar) .search,
.rstudio-themes-flat .themedPopupPanel .search {
  background-color: #1a1a1a !important; /* Near-black for input background */
  color: #f0f0f0 !important; /* Light gray for input text */
  border-radius: 0 !important; /* No rounded corners, as specified */
  border: 1px solid transparent;
}

.rstudio-themes-dark
  .gwt-TabLayoutPanelContent
  > div:nth-last-child(2):nth-child(4)
  .rstudio-themes-background
  .search {
  height: 18px;
}

.rstudio-themes-dark .search:-webkit-any(:focus, :focus-within) {
  outline: 1px solid rgba(50, 180, 255, 0.8); /* Neon blue outline for focus */
  outline-offset: -1px;
}

/* Scrollbars */
::-webkit-scrollbar {
  height: calc(17px * 10 / 14) !important;
}

::-webkit-scrollbar-thumb {
  border: none !important;
  border-radius: 0 !important;
  background-color: rgba(70, 70, 70, 0.4) !important; /* Darker gray for scrollbar thumb*/
}

::-webkit-scrollbar-thumb:hover {
  background-color: rgba(90, 90, 90, 0.7) !important; /* Slightly lighter on hover */
}

::-webkit-scrollbar-thumb:active {
  background-color: rgba(120, 120, 120, 0.4) !important; /*Even lighter for active*/
}

::-webkit-scrollbar,
::-webkit-scrollbar-corner,
::-webkit-scrollbar-track {
  background-color: transparent !important; /* Keep scrollbar track transparent */
}

::-webkit-scrollbar-track:vertical {
  box-shadow: inset 1px 0 0 0 #222; /* Dark separator color */
}

/* Menus */
.rstudio-themes-flat .themedPopupPanel,
.rstudio-themes-flat .popupMiddleCenter,
.rstudio-themes-flat .menuPopupMiddleCenter {
  color: #f0f0f0 !important; /* Light gray text for menu items*/
  background-color: #111 !important; /* Near black background for menus*/
  border-color: #333 !important; /* Darker border for menus */
}

.rstudio-themes-flat .themedPopupPanel ::-webkit-scrollbar:vertical {
  width: 10px !important;
}

.rstudio-themes-flat .themedPopupPanel ::-webkit-scrollbar-track:vertical {
  box-shadow: none;
}

.rstudio-themes-flat .gwt-MenuItem-selected {
  background-color: #005580 !important; /* Neon blue-ish background for selection */
}

.rstudio-themes-flat .gwt-MenuItemSeparator > .menuSeparatorInner {
  border-color: rgba(100, 100, 100, 0.4) !important; /* Slightly darker separator */
}

/* Terminal */
.terminal {
  background-color: #000; /* Pure black for terminal */
  color: #d0d0d0; /* Light gray for terminal text */
  font-feature-settings: "liga" 0;
  position: relative;
  user-select: none;
  -ms-user-select: none;
  -webkit-user-select: none;
}
.terminal.xterm-cursor-style-block.focus:not(.xterm-cursor-blink-on) .terminal-cursor {
  background-color: #d0d0d0; /* White cursor */
  color: #000; /* black cursor text*/
}
.terminal.focus.xterm-cursor-style-bar:not(.xterm-cursor-blink-on) .terminal-cursor::before,
.terminal.focus.xterm-cursor-style-underline:not(.xterm-cursor-blink-on) .terminal-cursor::before {
  content: "";
  position: absolute;
  background-color: #d0d0d0; /* White cursor */
}
.terminal:not(.focus) .terminal-cursor {
  outline: 1px solid #d0d0d0; /* white outline for unfocused cursor*/
  outline-offset: -1px;
}
.terminal .xterm-selection div {
