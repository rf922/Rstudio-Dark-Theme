/* --- Dialog Background ---- */
.gwt-DialogBox {
    background-color: #000000; /* Pure black */
    border: 1px solid #222; /* Dark border */
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.5);
}

/* Dialog title bar */
.dialogTopCenterInner {
    background-color: #111; /* Darker than pure black */
}

.dialogTopCenter .Caption {
    color: #ddd; /* Very light gray for title text */
    font-weight: bold;
	padding: 5px;
}

/* remove the white line below the title */
.dialogTopCenter {
    background-color: #111;
}

/* --- Tab list ---- */

.GNTYACMBEL {
    background-color: #000000;
    border-right: 1px solid #222;
}

/* Unselected tabs */
.GNTYACMBDL {
    background-color: #111;
    color: #888;
    border-bottom: 1px solid #222;
}

/* Selected tab */
.GNTYACMBDL.__rstudio_modal_first_focus,
.GNTYACMBDL.GNTYACMBHK {
    background-color: #333;
    color: #fff;
    border-bottom: 1px solid #444;

}

/* Tab hover and focus */
.GNTYACMBDL:hover,
.GNTYACMBDL:focus {
    background-color: #333;
    color: #fff;
}

/* Tab label text */
.GNTYACMBBL {
    color: inherit;
    white-space: nowrap;
}

/* Main dialog content area */
.dialogMiddleCenterInner {
    background-color: #000000;
    color: #eee;
}

/* Group labels (e.g., "R Sessions", "Workspace")*/
.gwt-Label.GNTYACMBKK.GNTYACMBAL {
    color: #aaa;
    font-weight: bold;
}

/* Individual setting label */
.gwt-Label {
    color: #eee;
}

/* Text Input Boxes*/
.gwt-TextBox {
    background-color: #222;
    color: #fff;
    border: 1px solid #333;
}

.gwt-TextBox-readonly {
    color: #888;
}

/* Button - General Style */
.gwt-Button {
    background-color: #222;
    color: #fff;
    border: 1px solid #333;
}


/* Button: Fix White text on buttons - Specific Target */
.gwt-Button .GNTYACMBC2 {
    color: black;
}


.gwt-Button:hover {
    background-color: #333;
}

/* Checkboxes and labels */
.gwt-CheckBox label {
    color: #ccc;
}

/* Dropdowns (ListBox)*/
.gwt-ListBox {
    background-color: #222;
    color: #fff;
    border: 1px solid #333;
}

/* Selected Dropdown Items */
.gwt-ListBox option:checked {
    background-color: #2e86c1;
}

/*Tab Panel */
.gwt-TabLayoutPanel {
    background-color: #000;
}

/* Tab panel labels */
.gwt-TabLayoutPanel .gwt-Label {
    color: #bbb;
}

.gwt-TabLayoutPanelTabs .gwt-TabLayoutPanelTab {
    border-bottom: 1px solid #222;
    background-color: #111;
}

.gwt-TabLayoutPanelTabs .gwt-TabLayoutPanelTab-selected {
    background-color: #333;
    color:#fff;
}

/* Unselected list options */
.GNTYACMBN2.GNTYACMBIR {
    background-color: black; /* Dark background for unselected options*/
    color: #888; /* muted color for unselected option text */
}


/* Selected list options */
.GNTYACMBN2[aria-selected="true"] {
  background-color: #333;
  color: #fff;
}


/* Hover for list options */
.GNTYACMBN2:hover{
  background-color: #333;
  color: #fff;
}


/* Hyperlink */
.rstudio-HyperlinkLabel,
.rstudio-HyperlinkLabel:link,
.rstudio-HyperlinkLabel:visited {
    color: #71b1e8; /* Soft blue */
    text-decoration: none;
}

.rstudio-HyperlinkLabel:hover,
.rstudio-HyperlinkLabel:active {
    color: #9fd5ff; /* Lighter blue on hover */
    text-decoration: underline;
}

/* Help button */
.rstudio-HelpButton {
    background-color: transparent;
}

